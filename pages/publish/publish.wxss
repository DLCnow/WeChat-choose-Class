.mui-bg{
  position: relative;
}



.keep-out{
  position: fixed;
  height: 100%;
  width: 100%;
  background-color: rgba(0,0,0,.6);
  z-index: 1111;
  top: 0;
  left: 0;
}
.keep-out-m{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  height: 30px;
  width: 80%;
  background-color: #fff;
  display: flex;
  align-items: center;
  border-radius: 5px;
  z-index: 1111;
}
.keep-select{
  width: 100%;
  height: 30px;
  display: flex;
  position: relative;
  align-items: center;
  z-index: 111;
}
.keep-out-all{
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  height: 180px;
  width: 80%;
  z-index: 1;
  display: flex;
  align-items: center;
  background-color: #fff;
  border-radius: 5px;
}
.keep-select-mo{
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  height: 180px;
  width: 80%;
  z-index: 1;
  display: flex;
  align-items: center;
}
.select-option-mo{
  color: grey;
  text-align: center;
  font-size: 14px;
  height: 30px;
  line-height: 30px;
}



.keep-select-flex{
  overflow:scroll;
  overflow-x:hidden;
  overflow-y:hidden;
  flex: 1;
}

.keep-select-flex-mo{
  overflow:scroll;
  overflow-x:hidden;
  overflow-y:hidden;
  flex: 1;
  height: 180px;
}
.select-option{
  text-align: center;
  font-size: 14px;
  height: 30px;
  line-height: 30px;
}





.keep-line{
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  height: 30px;
  width: 100%;
  border-top: 1px solid #eee;
  border-bottom: 1px solid #eee;
}


::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent
}