.p-red{
  font-size: 1.1rem !important;
  color: red !important;
}

.p-gray{
  font-size: 1.0rem !important;
  color: #828586 !important;
}

.p-title{
    font-size: 1rem !important;
    color: #404141 !important;
    font-weight: bold;
  
    /* line-height: 20rpx; */
}

.p-spec{
  text-indent: 2rem;
  font-size: 1rem !important;
  color: #828586 !important;
}



.p-value{
    font-size: 1rem !important;
    color: #0099ff !important;
    font-weight: normal;
}

.p-black-normal{
  font-size: 0.9rem !important;
  color: #404141 !important;
  position: inherit;
}

.mui-box{
  position: relative;
  margin-top: 20rpx;
  padding: 10rpx 30rpx;
}

.btn-dopush{
  border: 1rpx solid #e4e4e4;
  border-radius: 10rpx;
  padding:5rpx 25rpx;
  font-size: 1rem !important;
  background-color: #4e9fe3;
  margin:8rpx 40rpx;
  color:#fff;
}

.p-left{
  display: inline-block;
  line-height: 16.5px;
}

.pull-right{
  float: right;
  display: inline-block;
  margin-top: -20rpx;
}

.btn-doCall{
  font-size: 0.7rem !important;
  color: #fff;
  background-color: #4e9fe3;
  margin-left: 20rpx;
  margin-top: 16rpx;
  float: right;
}





.project-name{
  min-width: 1.8rem;
  display: block !important;
  line-height: 40rpx;
  min-height: 1.8rem;
  float: right;
  text-align: right;
  margin-top: 0 !important;
}

.pro-txt{
   min-height: 1.8rem;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.view-side{
  display: flex;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: #fff;
  border-top: 1px solid #eee;
  padding: 10rpx 0;
  opacity: .8;
}
.side{
  flex: 1;
}
.small-logo{
  display: block;
  margin: 0 auto;
  height: 28px;
  width: 28px
}
.side text{
  font-size: 20rpx;
  text-align: center;
  display: block;
  margin: 0 auto;
}
.mui-box{
  border-bottom: 0;
}
.v-mui-box{
  border-bottom: 0;
  height: 12rem;
}

.side-share{
  display: flex;
  align-items: center;
  justify-content: center;
}

.share-btn{
  padding: 0;
  margin: 0;
  background-color: transparent;
  border: 0;
  height: 70rpx;
  width: 70rpx;
}


.share-btn::after{ border: none; } 

.share-logo{
  height: 70rpx;
  width: 70rpx;
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: center;
}
.pull-image{
  height: 40rpx;
  width: 40rpx;

}



.view-phone{
  position: absolute;
  right: -5rpx;
  top: 50%;
  transform: translate(-50%,-50%);
  display: inline-block;
  height: 40rpx;
  width: 40rpx;
  margin-left:0;
  margin-top: 0;
}






.p-red{
  min-width: 18%;
}

.group-all{
  width: 100%;
  border-bottom: 1px solid #c1c1c1;
}
.group{
  width: 95%;
  display: flex;
  margin: 15rpx auto;
  min-height: 50rpx;
  align-items: center;
  justify-content: space-between;
}
.group-left{
  display: flex;
  align-items: center;
  min-height: 50rpx;
  font-weight: 800;
  min-width: 200rpx;
}
.group-right{
  display: flex;
  align-items: center;
  justify-content: center;
  min-height: 50rpx;
  color: #0099ff ;
}

.group-right>view{
  display: flex;
  align-items: center;
 color: #0099ff ;
}